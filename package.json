{
  "name": "fysx",
  "version": "0.0.3",
  "description": "Extendable 2D Physics Library for the Web & Node",
  "keywords": [ "2d", "physics", "library", "extendable", "browser", "web", "node" ],
  "main": "build/index.js",
  "scripts": {
    "build": "npx tsc",
    "watch": "npx tsc -w",
    "dist": "npx webpack --mode production",
    "publish": "npm run build && npm run dist && npm publish",
    "example:console": "npx ts-node ./examples/console/index.ts",
    "example:pixi": "npx webpack-dev-server --config examples/pixi/webpack.config.js"
  },
  "author": "Jesper Gustafsson",
  "homepage": "https://github.com/TBubba/fysx",
  "repository": "github:TBubba/fysx",
  "license": "MIT",
  "devDependencies": {
    "pixi.js": "^5.0.0-rc.2",
    "ts-loader": "5.3.3",
    "ts-node": "^8.0.2",
    "typescript": "3.3.3",
    "webpack": "4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0"
  },
  "dependencies": {}
}
